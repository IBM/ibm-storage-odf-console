{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2016",
    "jsx": "react",
    "allowJs": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "paths": {
      "@console/dynamic-plugin-sdk/api": [
        "node_modules/@console/dynamic-plugin-sdk/lib/api/api"
      ],
      "@console/dynamic-plugin-sdk/provisional": [
        "node_modules/@console/dynamic-plugin-sdk/lib/api/provisional"
      ],
      "@console/dynamic-plugin-sdk/webpack": [
        "node_modules/@console/dynamic-plugin-sdk/lib/webpack"
      ]
    }
  },
  "include": ["src"]
}
